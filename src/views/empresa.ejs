                    </div>
                    <div class="labelinput">                    
                        <label for="">CIF</label>                    
                        <input type="text"  disabled id="CIFTrabajador" name="CIFTrabajador" >                  
                    </div>                                        
                </div> 
                <div class="rowlabelinput">
                    <div class="labelinput">
                        <label for="centro">Lista de Centros</label>
                        <select id="cmbcentro" name="cmbcentro" > </select>
                    </div> 
                    <div class="labelinput">
                        <label for="nifnie">NIF/NIE</label>
                        <input type="text" id="nifnie" name="nifnie" >
                    </div>  
                </div>
                <div class="rowlabelinput"> 
                    <div class="labelinput">
                        <label for="nombreTrabajador">Nombre</label>
                        <input type="text" id="nombreTrabajador" name="nombreTrabajador" >
                    </div>
                    <div class="labelinput">
                        <label for="apellidosTrabajador">Apellidos</label>
                        <input type="text" id="apellidosTrabajador" name="apellidosTrabajador" >
                    </div>                   
                </div>
              
                <div class="rowlabelinput">
                    <div class="labelinput">
                        <label for="telefonoTrabajador">Telefono</label>
                        <input type="number" id="telefonoTrabajador" name="telefonoTrabajador" >
                    </div>
                    <div class="labelinput">
                        <label for="emailTrabajador">Email Trabajador</label>
                        <input id="emailTrabajador" required name="emailTrabajador" >
                    </div>                          
                </div>               
            </div>
            <div class="btnInput">
                <label id="messageTrabajador" for=""></label>
                <div class="button">
