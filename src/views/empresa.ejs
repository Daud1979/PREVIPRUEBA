    <div class="listaCentros">
        <div class="headLista">
            <h4>LISTA DE TRABAJADORES EN TODA LA EMPRESA</h4>
            <input type="text" name="buscartrabajador" id="buscartrabajador" placeholder="Escriba el dni, nombre, apellido, centro">
        </div>
        <div class="listaCentro">
            <table id="tablaTrabajador">
                <thead>
                    <tr>
                        <th>Nº</th>
                        <th>Centro</th>
                        <th>NIF</th>
                        <th>Nombre</th>
                        <th>Apellidos</th>
                        <th>Email</th>
                        <th>Telefono</th>
                        <th>Alta</th>
                        <th>Estado</th>  
                        <th>Opciones</th>                     
                    </tr>
                </thead>
                <tbody id="tbodyT">                  
                
                </tbody>
            </table>
        </div>
    </div>
    <label id="updatelblTrabajador" ></label>
   
</div>
  <!--FIN TRABAJADORES-->
</div>
